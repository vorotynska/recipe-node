<h2>Users Table</h2>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Zip Code</th>
    </tr>
  </thead>
  <tbody>
    <% users.forEach(user=> { %>
      <tr>
        <td>
          <a href="<%= `/users/${user._id}` %>">
            <%= user.fullName %>
          </a>
        </td>
        <td>
          <%= user.email %>
        </td>
        <td>
          <%= user.zipCode%>
        </td>
        <td>
          <a href="<%=`/users/${user._id}/edit` %>">
            Edit
          </a>
        </td>
        <td>
          <form method="POST" action="<%=`/users/${user._id}/delete?_method=DELETE`%>">
            <button type="submit" onclick="return confirm('Are you sure you want to delete this record?')">Delete</button>
          </form>
        </td>
      </tr>
      <% }); %>
  </tbody>
</table>